import java.util.*;
import java.lang.*;
class Main
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number : ");
        int n=sc.nextInt();
        int fac=1;
        for(int i=1;i<=n;i++)
        {
            fac=fac*i;
        }
        System.out.println("The factorial of "+n+" is : "+fac);
    }
}
